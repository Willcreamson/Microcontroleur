void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
}

void loop() {
  // put your main code here, to run repeatedly: 
  
  // Partie A recentrage des valeurs du signal
  float moyenne;
  moyenne = 0.;
  for (int i = 0; i<1000; i++){
    moyenne += Signal[i];
  }
  moyenne = moyenne/1000;

  for (int i=0; i<1000; i++){
    Signal[i] = {(float)Signal[i]-moyenne};
  }

  // Partie B Calcul de la puissance instantanée fenêtre glissante
  
  for (int i = 0; i<1000; i++){
  Puiss_inst[i] = 0;
    for (int j=-K; j<K; j++){
      Puiss_inst[i] += (Signal[j]-moyenne)*(Signal[j]-moyenne);
      Puiss_inst[i] = divz * Puiss_inst[i];
    Serial.println("La puissance en dB est de :");
    Serial.println(10*log(Puiss_inst[i]));
  }
  // Partie C Comparaison avec le niveau de bruit
  if (10*log(Puiss_inst[i]) > 75){
    Serial.println("Attention bruit nuisible");
  }
  else if (10*log(Puiss_inst[i]) > 120){ 
    Serial.println("Attention bruit dangereux !");
  }
  }
}
